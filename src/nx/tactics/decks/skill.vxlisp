(func tactics-skillmap<-tactics : base/tactics
 [tactics : base/tactics]
 (let : base/tactics
  [skillmap : base/skillmap :=
   (skillmap-tactics tactics)]
  (copy tactics
   :skillmap skillmap))
 :doc "Returns a tactics updated with skillmap")
